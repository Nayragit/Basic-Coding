#include <iostream>
using namespace std;
int max(int arr[],int n){
    int ans = arr[0];
    for(int i = 1;i<n;i++){
    if(arr[i]>ans){
        ans = arr[i];
    }
}
    return ans;
}
int main() {
	int n;
	cin>>n;
	int arr[n];
	for(int i = 0;i<n;i++){
	    cin>>arr[i];
	}
	cout<<max(arr,n)<<" ";
	return 0;
}

INPUT : 5
         1 2 3 4 19
OUTPUT : 19
