#include <iostream>
using namespace std;
int sum(int arr[],int n){
    int ans = 0;              // to store sum
    for(int i = 0;i<n;i++){    // sum k liye loop hai iye
        ans = ans+arr[i];
    }
    return ans;
}

int main() {
	int n;
	cin>>n;
	int arr[n];
	for(int i = 0;i<n;i++){
	    cin>>arr[i];
	}
	cout<<sum(arr,n)<<" ";  // calling sum function
	return 0;
}

input : 5
        2 3 1 2 3
 output : 11
